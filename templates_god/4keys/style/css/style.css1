img,
iframe {
    max-width: 100%!important;
}
body > *,
a,
input {
    margin: 0;
    outline: none;
    font-family: 'PT Sans', sans-serif;
}
body {
    min-width: 1024px;
    font-family: 'PT Sans', sans-serif;
    background-attachment: fixed;
}
.wrapper {
    width: 1024px;
    margin: 0 auto;
    z-index: 99;
    position: relative;
}


.inputs {
	position:absolute;
	top:45px;
	right:80px;
	z-index:100;
}

.inputs_info {
	width:360px;
	padding:20px;
	color:#fff;
	position:fixed;
	right: 50px;
}

.inputs em {
	display:block;
	width:280px;
}

.inputs_green {
	background:#373925;
	border:1px solid #829b34;
}

.inputs_red {
	background:#4b2424;
	border:1px solid #e93434;
}

.close {
	background:url(/templates_god/4keys/style/images/close.png)/***css-loader:: can't load recource***/ no-repeat 0 0;
	width:12px;
	height:12px;
	position:absolute;
	top:20px;
	right:20px;
	cursor:pointer;
	transition:all 0.5s ease;
}

.close:hover {
	opacity:0.5;
}



.wrapper:after {
    content: "";
    display: table;
    clear: both;
}
strong, b {
    font-weight: bold;
}
a {
    text-decoration: none;
    color: #1B1B1B;
}
.clear {
    overflow: hidden;
    width: 100%;
    clear: both;
    height: 0;
}
.rpsn {
    right: 0 !important;
}
#goods {
    width: 806px;
    float: left;
}
body header .wrapper {
    border-radius: 0 0 3px 3px;
}


#top_menu {
    height: 12px;
    border-bottom: 1px solid #f5f5f5;
    padding: 12px 25px;
    background: #fff
}
#top_menu li a:hover {
    color: #4886c1;
}
#top_menu li a {
    color: #747474;
    text-decoration: none;
    text-transform: uppercase;
    font-size: 11px;
    line-height: 11px;
    transition: all .2s ease;
    display: block
}
#top_menu li:nth-last-child(2) {
    margin-right: 0
}
#top_menu li:last-child {
    position: relative;
    margin-right: 20px!important
}
#top_menu li:last-child:before {
    content: '';
    width: 1px;
    height: 16px;
    background: #f5f5f5;
    position: absolute;
    right: -10px;
    top: 1px
}
#top_menu li:last-child a:hover {
    opacity: .8
}
#top_menu li:last-child a {
    margin-right: 0;
    float: left;
    height: 12px;
    width: 17px;
    border: 0!important
}
#top_menu li:last-child,
#top_menu li:nth-last-child(2) {
    float: left;
	width: 65px;
}
#top_menu li {
    vertical-align: top;
    margin-right: 25px;
    display: inline-block
}
.logo {
    display: block;
    float: left;
    width: 206px;
    height: 70px;
    background: url(/templates_god/4keys/style/img/logo.png) no-repeat center center
}
.sell_date {
    text-decoration: none;
    font-size: 10px;
    display: block;
    position: absolute;
    left: 5px;
    bottom: 0;
    color: #fff;
    line-height: 25px;
    font-weight: 700;
    z-index: 3;
    text-shadow: 1px 1px 5px #000
}
#mainpage .wrapper {
    padding: 17px 20px 0;
    width: 984px
}
#mainpage > .wrapper,
header > .wrapper {
    background: #fff
}
.clearfix {
    clear: both
}


.top-nav.letters {
    border-top: 1px solid #f5f5f5;
    padding: 10px;
}
.top-nav-menu {
    height: 25px;
    overflow: hidden;
}
.top-nav-menu.letters li {
    float: left;
    margin: 0 6px;
    font-size: 11px;
    text-transform: uppercase;
    text-align: center
}
.top-nav-menu.letters li:first-child {
    margin-left: 0;
    margin-right: 4px;
}
.top-nav-menu.letters li:last-child {
    margin-right: 0;
    float: left;
}
.top-nav-menu.letters li.d:before {
    content: '';
    display: inline-block;
    margin: 7px 0 0 0;
    width: 1px;
    height: 10px;
    background: #b6b6b6;
}
.top-nav-menu.letters li a {
    font-size: 12px;
}
.top-nav-menu li a {
    color: #727272;
    text-decoration: none;
    display: block;
    height: 25px;
    line-height: 25px;
    padding: 0 10px;
    -moz-border-radius: 2px;
    -webkit-border-radius: 2px;
    border-radius: 2px
}
.top-nav-menu li a:hover {
    color: #fff;
    background: #212121
}
.top-nav-menu li a.blue {
    color: #fff;
    background: #0fc9f2;
    font-weight: 700
}
.top-nav-menu li a.blue:hover {
    background: #212121
}
.top-nav-menu li a.red {
    color: #fff;
    background: #f10545;
    font-weight: 700
}
.top-nav-menu li a.red:hover {
    background: #212121
}
.search-wrapper {
    transition: all .3s ease;
    float: left;
    margin-top: 18px;
    margin-right: 10px;
}
header form.focus input[type="text"] {
    color: #212121;
    border-color: #ffc1aa!important;
    background: #fff!important
}
header form.focus input[type="submit"] {
    border-color: #ffc1aa!important
}
#search_data {
    transition: all .3s ease;
    float: left;
    width: 515px;
    border-radius: 2px 0 0 2px;
    height: 34px;
    padding: 0 15px;
    line-height: 34px;
    border: 1px solid #cfcfcf;
    color: #a4a4a4;
    font-size: 13px;
    box-sizing: content-box;
    background: #fcfcfc;
}
#search_submit {
    transition: all .3s ease;
    float: left;
    background: #fff;
    text-align: left;
    cursor: pointer;
    width: 38px;
    height: 34px;
    border: 1px solid #cfcfcf;
    border-left: 0;
    color: #727272;
    font-size: 14px;
    box-sizing: content-box;
    border-radius: 0 2px 2px 0;
    background: url(/templates_god/4keys/style/img/search.png) no-repeat 12px 10px;
    padding: 0;
}
#search_submit:hover {
    background-color: #f8f8f8
}
.catalog {
    float: left;
    height: 36px;
    width: 210px;
    color: #fff;
    background: #cc181e;
    border-radius: 3px;
    margin-top: 18px;
    line-height: 36px;
    text-transform: uppercase;
    font-size: 12px;
    text-align: center;
}
.catalog:hover {
    background: #bd3232;
}


.promo {
    text-align: center;
    position: relative;
}
.promo-row {
    width: 1024px;
    margin: 20px auto;
    position: relative;
}
.promo-row img {
    width: 1024px;
    height: auto;
}
main {
    display: block;
    background: #fff;
    -moz-border-radius-topleft: 2px;
    -webkit-border-top-left-radius: 2px;
    border-top-left-radius: 2px;
    -moz-border-radius-topright: 2px;
    -webkit-border-top-right-radius: 2px;
    border-top-right-radius: 2px;
    padding: 10px;
}
main:after {
    content: "";
    display: table;
    clear: both;
}
#side-left {
    float: left;
    width: 188px;
    margin-right: 10px;
}
.block {
    width: 188px;
    margin-bottom: 12px;
    display: block;
}
.block .top {
    background: url(/templates_god/4keys/style/img/b-nav.png) no-repeat 0 0;
    height: 33px;
    overflow: hidden;
    line-height: 33px;
    display: block;
    color: #fff;
    font-size: 12px;
    padding-left: 28px;
    padding-right: 10px;
    text-transform: uppercase;
}
#side-left .block .cont {
    margin: 8px 0 0;
    font-size: 12px;
}
.b-nav li a {
    background: url(/templates_god/4keys/style/img/b-nav.png)/***css-loader:: can't load recource***/ no-repeat 0 -33px;
    padding-left: 20px;
    height: 21px;
    line-height: 21px;
    display: block;
    color: #1B1B1B;
}
.b-nav li:hover a {
    background-position: 0 -54px;
    color: #fff;
}
.counter img {
    padding-right: 12px;
    opacity: .5;
    -webkit-transition: opacity .25s ease-in-out;
    -moz-transition: opacity .25s ease-in-out;
    -ms-transition: opacity .25s ease-in-out;
    -o-transition: opacity .25s ease-in-out;
    transition: opacity .25s ease-in-out;
}
.counter:hover img {
    opacity: 1;
}



.main-tabs {
    background: #1b1b1b;
    float: left;
    display: table;
    position: relative;
    width: 556px;
    height: 32px;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
}
.main-tabs a {
    display: table-cell;
    text-align: center;
    vertical-align: middle;
    width: 33.33%;
    color: #fff;
    text-decoration: none;
    font-weight: bold;
    font-size: 11px;
    text-transform: uppercase;
}
.main-tabs a.active {
    background-color: #4886c1;
    cursor: default;
}
.main-tabs a:hover {
    background: #4886c1;
}
.main-tabs a:first-child {
    -webkit-border-top-left-radius: 3px;
    -webkit-border-bottom-left-radius: 3px;
    -moz-border-top-left-radius: 3px;
    -moz-border-bottom-left-radius: 3px;
    border-top-left-radius: 3px;
    border-bottom-left-radius: 3px;
}
.main-tabs a:last-child {
    -webkit-border-top-right-radius: 3px;
    -webkit-border-bottom-right-radius: 3px;
    -moz-border-top-right-radius: 3px;
    -moz-border-bottom-right-radius: 3px;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
}


