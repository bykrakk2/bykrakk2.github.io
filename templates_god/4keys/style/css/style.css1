.b-poster {
    float: left;
    margin-right: 10px;
    margin-bottom: 10px;
    display: block;
    width: 260px;
    max-height: 280px;
    position: relative;
    overflow: hidden;
    border: 1px solid #ececec;
    background: #fff;
}
.b-poster:nth-of-type(3n) {
    margin-right: 0;
}
.b-poster:hover {
    border: 1px solid #1B1B1B;
    background: #1B1B1B;
    color: #fff;
}
.b-poster:hover img {
    opacity: .3;
    -webkit-transition: opacity .25s ease-in-out;
    -moz-transition: opacity .25s ease-in-out;
    -ms-transition: opacity .25s ease-in-out;
    -o-transition: opacity .25s ease-in-out;
    transition: opacity .25s ease-in-out;
}
.b-poster:hover .more {
    opacity: 1;
}
.b-poster img {
    width: 260px;
    height: 140px;
    display: block;
    overflow: hidden;
}
.b-poster .more {
    width: 100px;
    height: 25px;
    line-height: 25px;
    display: block;
    position: absolute;
    left: 50%;
    top: 50%;
    margin: -22px 0 0 -50px;
    color: #fff;
    text-align: center;
    opacity: 0;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    text-transform: uppercase;
    border: 1px solid #fff;
    background: rgba(0, 0, 0, 0.53);
    font-size: 12px;
}

.b-poster .coast {
    position: absolute;
    z-index: 100;
    font-size: 12px;
    bottom: 0;
    padding: 0 10px;
    height: 36px;
    line-height: 36px;
    font-size: 12px;
}

